{
  "name": "formoose",
  "version": "2.0.0",
  "description": "Schema based run time validation engine, made to integrate back and front-end validations using Mongoose like schemas.",
  "private": false,
  "type": "module",
  "engine": {
    "node": "18.17.1"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "outputDir": "./dist",
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "ts-types": "tsc",
    "dev": "rimraf lib && tsup --watch",
    "build:dev": "rimraf lib && tsup",
    "build:rollup": "rollup -c",
    "build:tsup": "rimraf lib && env NODE_ENV=production tsup",
    "test": "jest",
    "start": "tsup --treeshake",
    "prepare": "echo \"prepared\"",
    "lint": "eslint \"src/**/*.ts{,x}\""
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:romulobordezani/formoose.git"
  },
  "keywords": [
    "mongoose",
    "schema",
    "form",
    "validation",
    "unified",
    "formoose",
    "react",
    "moose",
    "zod",
    "yup",
    "run time",
    "runtime"
  ],
  "author": "romulobordezani",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/romulobordezani/formoose/issues"
  },
  "homepage": "https://github.com/romulobordezani/formoose#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.3",
    "@swc/core": "^1.3.81",
    "@types/jest": "^25.1.4",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-plugin-import": "^2.28.1",
    "jest": "^29.6.4",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.35.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "tslib": "^2.6.2",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
